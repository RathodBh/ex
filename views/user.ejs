<%- include('header'); -%>
    <div class="container py-3">
        <div class="card">
            <div class="card-header text-light bg-primary">
                <%= students[0].firstName %>
                <%= students[0].lastName %>
            </div>
            <div class="card-body">
                <h5 class="card-title"></h5>
                <h6>
                    <span class="text-primary">contact Number:</span> <br>
                    <%= students[0].contactNumber %>
                </h6>
                <br>
                <h6>
                    <span class="text-primary">email Id:</span> <br>
                    <%= students[0].emailId %>
                </h6>
                <br>
                
                <h6 class="card-text">
                    <span class="text-primary">City:</span> <br>
                    <%= students[0].city %>
                </h6>
                <br>

                <h6 class="card-text">
                    <span class="text-primary">collage Name:</span> <br>
                    <%= students[0].clg %>
                </h6>
                <br>

                <a href="/show" class="btn btn-primary">Go Back</a>
            </div>
        </div>
        
    </div>

    <%- include('footer'); -%>